@model List<enQuery>
@{
    ViewBag.Title = "ContactUs";
    var sn = 1;
}
@section scripts{
    <script type="text/javascript">
        $('#tbl').DataTable()
    </script>
}
<div class="container-fluid">
    <!-- .row -->
    <div class="row">
        <div class="col-xs-12">
            <div class="with_border with_padding">
                <div class="row admin-table-filters">
                    <div class="col-lg-9">
                        <h4>Contact Queries</h4>
                    </div>
                </div>
                <!-- .row -->
                <div class="table-responsive">
                    <table class="table table-striped table-bordered" id="tbl">
                        <thead>
                            <tr>
                                <td class="media-middle text-center">
                                    S.no
                                </td>
                                <th>Name</th>
                                <th>Email</th>
                                <th>Phone</th>
                                <th>Subject</th>
                                <th>Message</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr class="item-editable">
                                    <td class="media-middle text-center">
                                        @sn
                                    </td>
                                    <td class="media-middle">
                                        <h5>
                                            @item.Name
                                        </h5>
                                    </td>
                                    <td class="media-middle">
                                        @item.E_Mail
                                    </td>
                                    <td>
                                        <div class="media">
                                            <div class="media-body">
                                                <h5>
                                                    @item.Contact
                                                </h5>
                                            </div>
                                        </div>
                                    </td>
                                    <td class="media-middle">
                                        @item.Subject
                                    </td>
                                    <td class="media-middle">
                                        @item.Message
                                    </td>
                                </tr>
                                sn++;
                            }
                        </tbody>

                    </table>
                </div>
                <!-- .table-responsive -->
            </div>
            <!-- .with_border -->
        </div>
        <!-- .col-* -->
    </div>
    <!-- .row -->
</div>
<!-- .container -->
