@model List<enContest>
@{
    ViewBag.Title = "Contest || List";
    var sn = 1;
}


<div class="row heading-bg bg-green">
    <div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
        <h5 class="txt-light">Contest</h5>
    </div>
    <!-- Breadcrumb -->
    <div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
        <ol class="breadcrumb">
            <li><a href="#">Contest</a></li>
            <li class="active"><span>List</span></li>
        </ol>
    </div>
    <!-- /Breadcrumb -->
</div>
<!-- /Title -->
<!-- Row -->
<div class="row">
    <div class="col-sm-12">
        <div class="panel panel-default card-view">
            <div class="panel-heading">
                <div class="pull-left">
                    <h6 class="panel-title txt-dark">Contest Table</h6>
                </div>
                <div class="clearfix"></div>
            </div>
            <div class="panel-wrapper collapse in">
                <div class="panel-body">
                    <div class="table-wrap">
                        <div class="table-responsive">
                            <table id="datable_1" class="table table-hover display  pb-30">
                                <thead>
                                    <tr>
                                        <td>S.No</td>
                                        <th>Description</th>
                                        <th>Start Date</th>
                                        <th>End Date</th>
                                        <th>Created On</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tfoot>
                                    <tr>
                                        <td>S.No</td>
                                        <th>Description</th>
                                        <th>Start Date</th>
                                        <th>End Date</th>
                                        <th>Created On</th>
                                        <th>Action</th>
                                    </tr>
                                </tfoot>
                                <tbody>
                                    @foreach (var item in Model)
                                    {
                                        <tr class="item-editable">
                                            <td class="media-middle text-center">
                                                @sn
                                            </td>
                                            <td class="media-middle">
                                                <h5>
                                                    @item.Description
                                                </h5>
                                            </td>
                                            <td class="media-middle">
                                                @item.StartDate
                                            </td>
                                            <td class="media-middle">
                                                @item.EndDate
                                            </td>
                                            <td class="media-middle">
                                                @item.InsertedOn
                                            </td>
                                            <td>
                                                <a class="btn yellow btn-outline sbold" href="/contest/update/@item.ID">
                                                    <i class="fa fa-pencil-square-o" aria-hidden="true"></i>
                                                </a>
                                                <a class="btn red btn-outline sbold" href="/contest/delete/@item.ID">
                                                    <i class="fa fa-trash" aria-hidden="true"></i>
                                                </a>
                                                <a class="btn red btn-outline sbold" href="/contest/getContestStatus/@item.ID">
                                                    <i class="fa fa-file-o" aria-hidden="true"></i>
                                                </a>
                                            </td>
                                        </tr>
                                        sn++;
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

